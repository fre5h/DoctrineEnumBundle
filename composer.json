{
    "name": "fresh/doctrine-enum-bundle",
    "description": "Provides support of ENUM type for Doctrine2 in Symfony applications.",
    "keywords": ["bundle", "symfony", "doctrine", "entity", "enum", "type", "DBAL", "MySQL", "SQLite", "PostgreSQL", "MSSQL"],
    "homepage": "https://github.com/fre5h/DoctrineEnumBundle",
    "type": "symfony-bundle",
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Artem Genvald",
            "email": "genvaldartem@gmail.com",
            "role": "Creator"
        },
        {
            "name": "Community",
            "homepage": "https://github.com/fre5h/DoctrineEnumBundle/graphs/contributors",
            "role": "Contributors"
        }
    ],
    "support": {
        "email": "genvaldartem@gmail.com",
        "issues": "https://github.com/fre5h/DoctrineEnumBundle/issues"
    },
    "require": {
        "php": ">=7.1.3",
        "doctrine/common": "~2.8",
        "doctrine/dbal": "~2.6",
        "symfony/config": "4.0.x-dev",
        "symfony/dependency-injection": "4.0.x-dev",
        "symfony/doctrine-bridge": "4.0.x-dev",
        "symfony/form": "4.0.x-dev",
        "symfony/framework-bundle": "4.0.x-dev",
        "symfony/http-kernel": "4.0.x-dev",
        "symfony/validator": "4.0.x-dev",
        "twig/twig": "~2.4"
    },
    "require-dev": {
        "phpunit/phpunit": "6.4.*",
        "escapestudios/symfony2-coding-standard": "3.0.*"
    },
    "autoload": {
        "psr-4": {
            "Fresh\\DoctrineEnumBundle\\": ""
        },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Fresh\\DoctrineEnumBundle\\Tests\\": "Tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "6.x-dev"
        }
    }
}
